{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "python bot.py",
    "environmentVariables": {
      "BOT_TOKEN": {
        "fromEnvironmentVariable": "BOT_TOKEN"
      },
      "PAYMENT_PROVIDER_TOKEN": {
        "fromEnvironmentVariable": "PAYMENT_PROVIDER_TOKEN"
      }
    }
  }
}
